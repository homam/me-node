<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">

    <script src="lib/zepto/zepto.js"></script>
      <script src="lib/hammer/hammer.js"></script>
    <script src="lib/three/three.min.js"></script>
    <script src="lib/three/tween.min.js"></script>
    <script src="lib/three//renderers/CSS3DRenderer.js"></script>



    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

        <article id="viewport">
            <section id="px" data-pos="[-1,0,0]" data-rot="[0,0.5,0]" data-look-at="[180,0]">
                <div>
                    <div class="content">
                        <h1>Homam Hosseini</h1>

                    </div>
                </div>
            </section>
            <section id="nx" data-pos="[1,0,0]" data-rot="[0,-0.5,0]" data-look-at="[0,0]">
                <div>
                    <div class="content">
                        <h1>2013</h1>
                    </div>
                </div>
            </section>
            <section id="nx_2" data-pos="[2,0,0]" data-rot="[0,-0.5,0]" data-look-at="[0,0]">
                <div>
                    <div class="content">
                        <h1>2012</h1>
                    </div>
                </div>
            </section>
            <section id="py" data-pos="[0,1,0]" data-rot="[-0.5,1,0.5]" data-look-at="[90,90]">
                <div><span>POS Y</span></div>
            </section>
            <section id="ny" data-pos="[0,-1,0]" data-rot="[-0.5,0,-0.5]" data-look-at="[0,-90]">
                <div><span>NEG Y</span></div>
            </section>
            <section id="pz" data-pos="[0,0,1]" data-rot="[0,1,0]" data-look-at="[90,0]">
                <div><span>POS Z</span></div>
            </section>
            <section id="nz" data-pos="[0,0,-1]" data-rot="[0,0,0]" data-look-at="[-90,0]">
                <div><span>NEG Z</span></div>
            </section>
 		</article>


  <script src="javascripts/index.js"></script>
  </body>
</html>
